pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('Clone Public Repo')
        {
            steps {
                git branch: 'main',
                    url: 'https://github.com/waldbaan/ITM-easy-devops-applicaite.git'
            }
        }
        stage('Execute')
        {
            steps{
                bat 'dotnet'
            }
        }
    }
}

